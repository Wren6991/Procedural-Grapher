<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="proceduralgrapherDialog">
		<title>Procedural Grapher</title>
		<size>-1,-1</size>
		<style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER</style>
		<object class="wxBoxSizer" variable="BoxSizer1" member="yes">
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="no">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TXTEXPR" variable="txtExpr" member="yes">
							<value>y = x^3 - x</value>
							<font>
								<size>10</size>
								<style>normal</style>
								<weight>normal</weight>
								<underlined>0</underlined>
								<family>swiss</family>
								<face>Consolas</face>
							</font>
							<style>wxTE_PROCESS_ENTER|wxTE_PROCESS_TAB|wxTE_MULTILINE|wxTE_CHARWRAP|wxSUNKEN_BORDER|wxVSCROLL</style>
							<handler function="Tokenize" entry="EVT_TEXT" />
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>2</option>
					</object>
					<object class="sizeritem">
						<object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="yes">
							<object class="notebookpage">
								<object class="wxTextCtrl" name="ID_TXTOUTPUT" variable="txtOutput" member="yes">
									<value>Text</value>
									<pos>-89,-653</pos>
									<bg>wxSYS_COLOUR_HIGHLIGHTTEXT</bg>
									<font>
										<size>10</size>
										<style>normal</style>
										<weight>normal</weight>
										<underlined>0</underlined>
										<family>swiss</family>
										<face>Consolas</face>
									</font>
									<style>wxTE_MULTILINE|wxTE_READONLY|wxSUNKEN_BORDER|wxVSCROLL</style>
									<handler function="OntxtOutputText" entry="EVT_TEXT" />
								</object>
								<label>Output</label>
							</object>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
									<pos>159,12</pos>
									<object class="wxCheckBox" name="ID_CHECKBOX1" variable="chk3D" member="yes">
										<label>3D</label>
										<pos>8,8</pos>
										<handler function="Onchk3DClick" entry="EVT_CHECKBOX" />
									</object>
									<object class="wxCheckBox" name="ID_CHECKBOX2" variable="chkGrid" member="yes">
										<label>Grid</label>
										<checked>1</checked>
										<pos>8,32</pos>
										<handler function="OnchkGridClick" entry="EVT_CHECKBOX" />
									</object>
								</object>
								<label>View</label>
							</object>
							<object class="notebookpage">
								<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
									<pos>162,14</pos>
									<object class="wxBoxSizer" variable="BoxSizer4" member="no">
										<object class="sizeritem">
											<object class="wxButton" name="ID_BUTTON1" variable="btnStartStopTime" member="yes">
												<label>Start Time</label>
												<handler function="OnbtnStartStopTimeClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
										<object class="sizeritem">
											<object class="wxButton" name="ID_BUTTON2" variable="btnResetTime" member="yes">
												<label>Reset Time</label>
												<handler function="OnbtnResetTimeClick" entry="EVT_BUTTON" />
											</object>
											<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
											<border>5</border>
											<option>1</option>
										</object>
									</object>
								</object>
								<label>Time</label>
							</object>
						</object>
						<flag>wxTOP|wxBOTTOM|wxLEFT|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxGLCanvas" name="ID_GLCANVAS1" variable="GLCanvas1" member="yes">
					<mDepthSize>16</mDepthSize>
					<size>300,300</size>
					<handler function="OnGLCanvas1LeftDown" entry="EVT_LEFT_DOWN" />
					<handler function="OnGLCanvas1LeftUp" entry="EVT_LEFT_UP" />
					<handler function="OnGLCanvas1MiddleDown" entry="EVT_MIDDLE_DOWN" />
					<handler function="OnGLCanvas1MiddleUp" entry="EVT_MIDDLE_UP" />
					<handler function="OnGLCanvas1MiddleDown" entry="EVT_RIGHT_DOWN" />
					<handler function="OnGLCanvas1MiddleUp" entry="EVT_RIGHT_UP" />
					<handler function="OnGLCanvas1MouseMove" entry="EVT_MOTION" />
					<handler function="OnGLCanvas1MouseWheel" entry="EVT_MOUSEWHEEL" />
					<handler function="OnGLCanvas1Resize" entry="EVT_SIZE" />
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="Timer1" member="yes">
			<interval>25</interval>
			<handler function="OnTimer1Trigger" entry="EVT_TIMER" />
		</object>
		<object class="wxStopWatch" variable="StopWatch1" member="yes" />
	</object>
</wxsmith>
